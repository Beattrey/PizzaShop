<h3>Корзина:</h3>
<br>
<table class="table table-bordered">
  <thead>
  <tr>
    <th>Наименование</th>
    <th>Стоимость</th>
    <th>Количество</th>
  </tr>
  </thead>
  <tbody>
  <% @items.each do |item| %>
  <tr>
    <td><%= item[0].title %></td>
    <td><%= item[0].price %></td>
    <td><%= item[1] %></td>
  </tr>
  <% end %>
  </tbody>
</table>

